{
	"name": "mix-match",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
		"dev": "next dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
		"lint:prettier": "prettier --check --ignore-path .gitignore .",
		"lint:typescript": "tsc",
		"prepare": "husky install",
		"start": "next start"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"overrides": {
		"autoprefixer": "10.4.5"
	},
	"dependencies": {
		"@heroicons/react": "^1.0.6",
		"@next/bundle-analyzer": "^12.2.0",
		"@prisma/client": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "10.4.5",
		"clsx": "^1.1.1",
		"cookie": "^0.5.0",
		"next": "^12.2.0",
		"next-compose-plugins": "^2.2.1",
		"postcss-import": "^14.1.0",
		"postcss-preset-env": "^7.7.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.53.0",
		"swr": "^1.3.0",
		"tailwindcss": "^3.1.4"
	},
	"devDependencies": {
		"@tsconfig/next": "^1.0.2",
		"@types/cookie": "^0.5.1",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.18.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^12.2.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-etc": "^2.0.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^8.0.1",
		"nano-staged": "^0.8.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"prisma": "^4.0.0",
		"typescript": "^4.7.4"
	},
	"nano-staged": {
		"*": "prettier --write --ignore-unknown",
		"*.{ts,tsx,js,jsx}": "eslint --fix"
	}
}
